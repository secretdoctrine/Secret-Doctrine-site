%div{id: 'page_container'}
  = link_to page.book.name + ' (' + page.url_name + ')', book_page_path(page.book.id, page.url_name, highlight_words: WordsConverter.convert(page.words.page_text))
  %br
  %iframe{src: "#{book_page_external_page_content_path(page.book, page, page.html_content, search_text: params[:search_text])}"}